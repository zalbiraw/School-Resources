#include<stdio.h>

#define MEDIAN(x, y, z) ((((x)>(y)&&(x)<(z))||((x)<(y)&&(x)>(z))) ? (x) \
		: (((y)>(x)&&(y)<(z))||((y)<(x)&&(y)>(z))) ? (y) : (z))

int main(void)
{
	printf("%d", MEDIAN(3+4, 6-1, 4));
	return 0;
}

#include<stdio.h>
#include<string.h>

#define LEN 10

int main(main)
{
	char sl [LEN], s1 [LEN], s2 [LEN];
	strcpy(sl, "computer");
	strcpy(s2, "science") ;
	if (strcmp(sl, s2) < 0)
		strcat (s1, s2);
	else
		strcat (s2, sl);
	s1[strlen(s1)-6] = '\0';puts(s1);
	return 0;
}
